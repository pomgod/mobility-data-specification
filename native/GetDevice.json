{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "device": {
        "properties": {
          "device_id": {
            "examples": ["3c9604d6-b5ee-11e8-96f8-529269fb1459"],
            "format": "uuid",
            "pattern": "^([0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})$",
            "type": "string"
          },
          "mfgr": {
            "type": ["null", "string"]
          },
          "model": {
            "type": ["null", "string"]
          },
          "propulsion": {
            "items": {
              "enum": ["combustion", "electric", "electric_assist", "human", "hybrid"],
              "type": "string"
            },
            "type": "array"
          },
          "provider_id": {
            "examples": ["3c9604d6-b5ee-11e8-96f8-529269fb1459"],
            "format": "uuid",
            "pattern": "^([0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})$",
            "type": "string"
          },
          "recorded": {
            "type": "number"
          },
          "status": {
            "anyOf": [
              {
                "enum": ["available", "elsewhere", "inactive", "removed", "reserved", "trip", "unavailable"],
                "type": "string"
              },
              {
                "type": "null"
              }
            ]
          },
          "type": {
            "enum": ["bicycle", "car", "recumbent", "scooter"],
            "type": "string"
          },
          "vehicle_id": {
            "type": "string"
          },
          "year": {
            "type": ["null", "number"]
          }
        },
        "type": "object"
      },
      "version": {
        "enum": ["0.1.0"],
        "type": "string"
      }
    },
    "type": "object"
  }
